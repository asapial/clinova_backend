model Specialty{
    
    id String @id @default(uuid())

    title String
    icon String?

    createdAt DateTime @default(now())
    updatedAt DateTime? @updatedAt

    isDeleted Boolean @default(false)
    deletedAt DateTime?

    @@index([isDeleted], name: "idx_specialty_isDeleted")
    @@index([title], name: "idx_specialty_title")
    @@map("specialties")
}